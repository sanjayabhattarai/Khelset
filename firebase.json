{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
      {
        "source": "/home/**",
        "destination": "/home/index.html"
      },
      {
        "source": "/home",
        "destination": "/home/index.html"
      },
      {
        "source": "/about/**",
        "destination": "/about/index.html"
      },
      {
        "source": "/about",
        "destination": "/about/index.html"
      },
      {
        "source": "/contact/**",
        "destination": "/contact/index.html"
      },
      {
        "source": "/contact",
        "destination": "/contact/index.html"
      },
      {
        "source": "/nplseason2/**",
        "destination": "/nplseason2/index.html"
      },
      {
        "source": "/nplseason2",
        "destination": "/nplseason2/index.html"
      },
      {
        "source": "/privacy-policy/**",
        "destination": "/privacy-policy/index.html"
      },
      {
        "source": "/privacy-policy",
        "destination": "/privacy-policy/index.html"
      },
      {
        "source": "/terms/**",
        "destination": "/terms/index.html"
      },
      {
        "source": "/terms",
        "destination": "/terms/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|webp|svg|ico)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=7200"
          }
        ]
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "khelset-new",
          "appId": "1:862681026576:android:3be9499b4c5bef091cba95",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "khelset-new",
          "configurations": {
            "android": "1:862681026576:android:3be9499b4c5bef091cba95"
          }
        }
      }
    }
  }
}
